#!/usr/bin/env python3

import os
import sys

sys.path.append(os.path.dirname(os.path.dirname(os.path.realpath(__file__))))

from cloud.command import Command

def main(args):
    cmd = Command()
    if len(args) == 1:
        cmd._help()
    else:
        cmd.run(args[1], args[2:])

if __name__ == "__main__":
    main(sys.argv)
